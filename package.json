    "@langchain/groq": "^0.1.0",
    "@langchain/langgraph": "^0.2.0",
    "@langchain/openai": "^0.3.0",
    "chalk": "^5.3.0",
    "dotenv": "^16.4.0",
    "express": "^4.18.2",
    "ora": "^8.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.0.0",
    "tsx": "^4.21.0",
